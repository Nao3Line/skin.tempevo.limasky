<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
    <name>TopAlbums</name>
    <match>all</match>
    <group>albums</group>
    <rule field="playcount" operator="greaterthan">
        <value>0</value>
    </rule>
    <order direction="descending">random</order>
</smartplaylist>
