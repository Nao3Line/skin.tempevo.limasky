<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="1105">
	<!-- only for LimaSky TabFriend -->
	<visible>Skin.HasSetting(LimaSkyTabAktif)</visible>
	<zorder>110</zorder>
	<controls>
        <control type="group">
			<visible>!Skin.HasSetting(TurnOffStatusBar)</visible>
			<visible>![Window.isActive(1106)|Window.isActive(1108)|Window.isActive(shutdownmenu)]</visible>
			<visible>![Window.isActive(Home)+Skin.String(HideStatusBaronHome)]</visible>
			<visible>![Skin.HasSetting(MuVizSettoSimpleOSD) + Window.isActive(visualisation) + !Window.isActive(musicosd)]</visible>
			<visible>![Window.isActive(VideoFullScreen.xml)+![Window.isActive(videoosd)|Window.isActive(fullscreeninfo)|Window.isActive(gameosd)]]</visible>
			<include>fadeInOutVisibleDelay</include>
			<!--<include>TaskBarandTimeDisplay</include>-->
		</control>
	</controls>
</window>
